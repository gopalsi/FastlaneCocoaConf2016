fastlane_version "1.66.0"

default_platform :ios

platform :ios do
  before_all do
  end

  desc "This lane is designed to test and then push to git remote"
  lane :test_and_push do
    scan
  end

  after_all do |lane|
  	if lane == :test_and_push
      push_to_git_remote
    end
  end

  error do |lane, exception|
  end
end